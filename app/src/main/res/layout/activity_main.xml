<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/fbbutton"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="21dp"
        android:contentDescription="@string/facebook"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/circle_facebook" />

    <ImageButton
        android:id="@+id/instbutton"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/fbbutton"
        android:layout_toRightOf="@+id/fbbutton"
        android:contentDescription="@string/instagram"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/circle_instagram" />

    <ImageButton
        android:id="@+id/telbutton"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/instbutton"
        android:layout_toRightOf="@+id/instbutton"
        android:contentDescription="@string/telegram"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/circle_telegram" />

    <ImageButton
        android:id="@+id/kakbutton"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/telbutton"
        android:layout_toRightOf="@+id/telbutton"
        android:contentDescription="@string/kakaotalk"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/circle_kakaotalk" />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="100dp"
        android:id="@+id/calllogs">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>


    </ScrollView>

</RelativeLayout>
